<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/carFragment">

  <fragment
      android:id="@+id/carFragment"
      android:name="com.dev.favoritecar.car.CarFragment"
      android:label="Car"
      tools:layout="@layout/car_fragment">
    <action
        android:id="@+id/action_carFragment_to_carStoreFragment"
        app:destination="@id/carStoreFragment" />
  </fragment>
  <fragment
      android:id="@+id/carStoreFragment"
      android:name="com.dev.favoritecar.carstore.CarStoreFragment"
      android:label="Car Catalogue"
      tools:layout="@layout/car_store_fragment" />
</navigation>